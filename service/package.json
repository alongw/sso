        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "tsx": "^4.7.0",
        "typescript": "^5.1.3",
        "webpack": "^5.90.3",
        "webpack-cli": "^5.1.4",
        "webpack-node-externals": "^3.0.0"
